{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["id", "body", "postId", "user"],
  "properties": {
    "id": {
      "type": "number"
    },
    "body": {
      "type": "string"
    },
    "postId": {
      "type": "number"
    },
    "user": {
      "type": "object",
      "required": ["id", "username"],
      "properties": {
        "id": {
          "type": "number"
        },
        "username": {
          "type": "string"
        }
      }
    }
  }
}
